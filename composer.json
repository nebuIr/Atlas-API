{
  "name": "xnebulr/atlas-api",
  "type": "project",
  "description": "API for No Man's Sky news, releases and game version",
  "require": {
    "php": "^8.0",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-mysqli": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "laminas/laminas-diactoros": "^2.11",
    "league/fractal": "^0.20.1",
    "middlewares/trailing-slash": "^2.0",
    "php-di/php-di": "^6.4",
    "simplehtmldom/simplehtmldom": "^2.0@RC",
    "slim/slim": "^4.10",
    "vlucas/phpdotenv": "^5.4",
    "phpfastcache/phpfastcache": "^9.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "AtlasAPI\\": "app/"
    }
  }
}
